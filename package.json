{
  "name": "seed-images",
  "version": "4.4.1",
  "description": "Angular component for Seed image discovery",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all --parallel ngserve test lint",
    "ngserve": "ng serve --host 0.0.0.0 --port 8080 --disable-host-check",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rimraf .tmp && rimraf dist",
    "transpile": "ngc -p ./src/tsconfig.build.json",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/seed-images.umd.js --screw-ie8 --compress --mangle--comments--output dist/bundles/seed-images.umd.min.js",
    "copy": "cpx './package-dist.json' dist && renamer --find 'package-dist.json' --replace 'package.json' ./dist/* && cpx './readme.md' dist",
    "build": "npm run clean && npm run transpile && npm run package && npm run minify && npm run copy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngageoint/seed-images.git"
  },
  "bugs": {
    "url": "https://github.com/ngageoint/seed-images/issues"
  },
  "homepage": "https://github.com/ngageoint/seed-images#readme",
  "author": "Applied Information Sciences",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/animations": "^7.0.2",
    "@angular/common": "^7.0.2",
    "@angular/compiler": "^7.0.2",
    "@angular/core": "^7.0.2",
    "@angular/forms": "^7.0.2",
    "@angular/http": "^7.0.2",
    "@angular/platform-browser": "^7.0.2",
    "@angular/platform-browser-dynamic": "^7.0.2",
    "@angular/router": "^7.0.2",
    "clipboard": "^2.0.0",
    "core-js": "^2.5.3",
    "font-awesome": "^4.7.0",
    "js-beautify": "^1.7.5",
    "lodash": "^4.17.5",
    "primeicons": "^1.0.0-beta.10",
    "primeng": "^6.1.4",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "^7.0.4",
    "@angular/compiler-cli": "^7.0.2",
    "@angular/language-service": "^7.0.2",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~8.9.4",
    "codelyzer": "^4.2.1",
    "cpx": "^1.5.0",
    "jasmine-core": "^2.99.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.2",
    "protractor": "^5.2.1",
    "renamer": "^0.6.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.52.1",
    "ts-node": "^4.0.1",
    "tslint": "^5.8.0",
    "typescript": "^3.1.6",
    "uglify-js": "^3.2.2",
    "webpack": "^4.20.1"
  }
}
