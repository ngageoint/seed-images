{
  "name": "@ngageoint/seed-images",
  "version": "5.0.1",
  "description": "Angular component for Seed image discovery",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all --parallel ngserve lint",
    "ngserve": "ng serve --host 0.0.0.0 --port 8080 --disable-host-check",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rimraf .tmp && rimraf dist",
    "transpile": "ngc -p ./src/tsconfig.build.json",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/seed-images.umd.js --screw-ie8 --compress --mangle--comments--output dist/bundles/seed-images.umd.min.js",
    "copy": "cp './package-dist.json' dist && renamer --find 'package-dist.json' --replace 'package.json' ./dist/* && cp './readme.md' dist",
    "build": "npm run clean && npm run transpile && npm run package && npm run minify && npm run copy",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngageoint/seed-images.git"
  },
  "bugs": {
    "url": "https://github.com/ngageoint/seed-images/issues"
  },
  "homepage": "https://github.com/ngageoint/seed-images#readme",
  "author": "Applied Information Sciences",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "clipboard": "^2.0.4",
    "core-js": "^2.6.11",
    "font-awesome": "^4.7.0",
    "js-beautify": "^1.10.3",
    "lodash": "^4.17.15",
    "primeicons": "^2.0.0",
    "primeng": "^8.1.1",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "^8.3.25",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "^3.5.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.149",
    "@types/node": "^11.13.23",
    "codelyzer": "^5.2.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.3",
    "renamer": "^1.1.4",
    "rimraf": "^2.7.1",
    "rollup": "^0.52.1",
    "standard-version": "^8.0.1",
    "ts-node": "~8.1.0",
    "tslint": "~5.16.0",
    "typescript": "^3.5.3"
  }
}
